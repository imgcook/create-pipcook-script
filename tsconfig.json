{
  "compilerOptions": {
    "resolveJsonModule": true,
    "composite": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "noImplicitAny": true,
    "incremental": true,
    "module": "commonjs",
    "target": "es6",
    "lib": [ "es6" ],
    "skipLibCheck": true,
    "sourceMap": true,
    "esModuleInterop": false
  },
  "exclude": [
    "dist"
  ]
}
